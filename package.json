{
  "name": "daily-image-discord-bot",
  "displayName": "daily Image Bot",
  "version": "1.0.0",
  "description": "A discord bot that displays a daily image from Google photos",
  "main": "out/index.js",
  "scripts": {
    "start": "npm run build && node ./out/index.js",
    "watch:start": "nodemon ./out/index.js",
    "watch:compile": "tsc -watch",
    "build": "tsc",
    "tsc": "tsc",
    "postinstall": "tsc",
    "heroku-prebuild": "npm install",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Josee9988/daily-image-discord-bot.git"
  },
  "author": {
    "name": "Jose Gracia Berenguer",
